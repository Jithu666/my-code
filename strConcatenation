public class strConcatenation {
    public static void main(String[] args) {
        String s = "githendra";
        String s1 = null;
        String s2 = s + s1;
        System.out.println(s.concat(s1));
        System.out.println("\nStrings " + s + " and " + s1 + " After concatenation are : " + s2);
    }
}
